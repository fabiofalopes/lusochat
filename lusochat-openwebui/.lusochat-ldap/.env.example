# API Keys
OPENAI_API_KEY=your-openai-api-key-here
GROQ_API_KEY=your-groq-api-key-here

# Ollama Configuration
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_DOCKER_TAG=latest

# WebUI Configuration
WEBUI_SECRET_KEY=your-very-strong-secret-key-here
WEBUI_NAME=Lusochat
WEBUI_DOCKER_TAG=main
OPEN_WEBUI_PORT=3000

# Logging Configuration
GLOBAL_LOG_LEVEL=DEBUG
AUDIO_LOG_LEVEL=DEBUG
COMFYUI_LOG_LEVEL=DEBUG
CONFIG_LOG_LEVEL=DEBUG
DB_LOG_LEVEL=DEBUG
IMAGES_LOG_LEVEL=DEBUG
MAIN_LOG_LEVEL=DEBUG
MODELS_LOG_LEVEL=DEBUG
OLLAMA_LOG_LEVEL=DEBUG
OPENAI_LOG_LEVEL=DEBUG
RAG_LOG_LEVEL=DEBUG
WEBHOOK_LOG_LEVEL=DEBUG

# Branding Configuration
CUSTOM_NAME=lusochat

# IMPORTANT: Must be true when using LDAP authentication
ENABLE_LOGIN_FORM=true

# LDAP Configuration
ENABLE_LDAP=true
LDAP_SERVER_HOST=your-ldap-server-host
LDAP_USE_TLS=false
LDAP_APP_DN=CN=YourAppUser,CN=Users,DC=yourdomain,DC=com
LDAP_APP_PASSWORD=your-ldap-app-password
LDAP_SEARCH_BASE=DC=yourdomain,DC=com
LDAP_SEARCH_FILTERS=
LDAP_ATTRIBUTE_FOR_USERNAME=sAMAccountName
LDAP_ATTRIBUTE_FOR_MAIL=mail
LDAP_SERVER_PORT=389
LDAP_DEBUG=true
LDAP_VERBOSE=true
LDAP_SERVER_LABEL=Your LDAP Server
LDAP_CIPHERS=ALL
LDAP_LOG_LEVEL=DEBUG
LDAP_CONNECTION_TIMEOUT=30
LDAP_REFERRAL_ENABLED=false
SQLALCHEMY_ECHO=True

# Cookie Security Settings
WEBUI_SESSION_COOKIE_SAME_SITE=lax
WEBUI_AUTH_COOKIE_SAME_SITE=lax
WEBUI_SESSION_COOKIE_SECURE=false
WEBUI_AUTH_COOKIE_SECURE=false